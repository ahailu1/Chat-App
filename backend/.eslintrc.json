{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "prefer-const":"warn",
        "no-unused-vars": "warn",
        "linebreak-style": "warn",
        "import/newline-after-import": "warn"
    }
}
